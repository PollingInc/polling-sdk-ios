  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The SDK uses the standard versioning scheme. The version string is in",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "inlineContent" : [
                {
                  "text" : "MAJOR.MINOR.PATCH",
                  "type" : "text"
                }
              ],
              "type" : "emphasis"
            },
            {
              "text" : " format.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "PROJECT_VERSION = 1.0.0"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The version string may contain extra info affixed to the end. The",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "extra info ",
              "type" : "text"
            },
            {
              "inlineContent" : [
                {
                  "text" : "must",
                  "type" : "text"
                }
              ],
              "type" : "strong"
            },
            {
              "text" : " begin with a hyphen and ",
              "type" : "text"
            },
            {
              "inlineContent" : [
                {
                  "text" : "must not",
                  "type" : "text"
                }
              ],
              "type" : "strong"
            },
            {
              "text" : " contain a",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "period. For a beta release one might set the version string as",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "follows:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "PROJECT_VERSION = 1.0.0-Beta1"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Or for a release candidate one might set the version string as",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "follows:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "PROJECT_VERSION = 1.0.0-RC1"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The extra version info ",
                  "type" : "text"
                },
                {
                  "inlineContent" : [
                    {
                      "text" : "must",
                      "type" : "text"
                    }
                  ],
                  "type" : "strong"
                },
                {
                  "text" : " begin with a hyphen.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Important",
          "style" : "important",
          "type" : "aside"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "The extra version info ",
                  "type" : "text"
                },
                {
                  "inlineContent" : [
                    {
                      "text" : "must not",
                      "type" : "text"
                    }
                  ],
                  "type" : "strong"
                },
                {
                  "text" : " contain a period.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Warning",
          "style" : "warning",
          "type" : "aside"
        },
        {
          "anchor" : "Identifying-Builds",
          "level" : 2,
          "text" : "Identifying Builds",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The build automatically embeds the version info in the SDK’s binary",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and outputs it to the console on the first use of the SDK.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Additionally the build automatically inserts the version info into",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "each of the frameworks’ ",
              "type" : "text"
            },
            {
              "code" : "Info.plist",
              "type" : "codeVoice"
            },
            {
              "text" : " and into the XCFrameworks’",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "Info.plist",
              "type" : "codeVoice"
            },
            {
              "text" : ", so that the SDK may be identified without running in an",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "app.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The format of the version info follows",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "Polling vMAJOR.MINOR.PATCH[-EXTRA]:BRANCH@SHORT_HASH:CONFIG[:dirty]"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "items" : [
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "major version number",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "MAJOR",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "minor version number",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "MINOR",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "patch version number",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "PATCH",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "extra version info",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "EXTRA",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "the current git branch",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "BRANCH",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "the git short hash",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "SHORT_HASH",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "the build configuration, one of ",
                        "type" : "text"
                      },
                      {
                        "code" : "Debug",
                        "type" : "codeVoice"
                      },
                      {
                        "text" : " or ",
                        "type" : "text"
                      },
                      {
                        "code" : "Release",
                        "type" : "codeVoice"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "CONFIG",
                    "type" : "text"
                  }
                ]
              }
            },
            {
              "definition" : {
                "content" : [
                  {
                    "inlineContent" : [
                      {
                        "text" : "the value ",
                        "type" : "text"
                      },
                      {
                        "code" : ":dirty",
                        "type" : "codeVoice"
                      },
                      {
                        "text" : " is appended if there are uncommitted changes or untracked files in the repo",
                        "type" : "text"
                      }
                    ],
                    "type" : "paragraph"
                  }
                ]
              },
              "term" : {
                "inlineContent" : [
                  {
                    "text" : "dirty",
                    "type" : "text"
                  }
                ]
              }
            }
          ],
          "type" : "termList"
        },
        {
          "inlineContent" : [
            {
              "text" : "Here is an examples of the full version string:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "Polling v1.0.0-RC1:master@c0532f3:Debug:dirty"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "The build automatically inserts the version string in the ",
              "type" : "text"
            },
            {
              "code" : "Info.plist",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "of every ",
              "type" : "text"
            },
            {
              "code" : ".framework",
              "type" : "codeVoice"
            },
            {
              "text" : " and ",
              "type" : "text"
            },
            {
              "code" : ".xcframework",
              "type" : "codeVoice"
            },
            {
              "text" : " under the key",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "code" : "POLSDKVersion",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
